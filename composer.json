{
    "name": "ahmetbedir/mvc",
    "type": "project",
    "description": "BDR Framework",
    "keywords": ["framework", "bdr"],
    "homepage": "https://github.com/ahmetbedir/bdrmvc",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet BEDÄ°R",
            "email": "a_bedir43@hotmail.com",
            "homepage": "http://ahmetbedir.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "filp/whoops": "^2.7",
        "illuminate/database": "^4.2",
        "windwalker/edge": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ahmetbedir\\Skeleton\\Core\\": "system/Core"
        },
        "classmap": [
            "app/Models"
        ],
        "files": [
            "app/routes.php",
            "system/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^1.14"
    }
}
